-- variables

-- flags
-- 0 = ground to stand on
-- 1 = ceilings and walls
-- 2 = floor that kills
-- 3 = ceiling that kills
-- 4 = destructable by bombs
-- spawn marker flags
-- 5 = power up
-- 6 = goal spawn
-- 7 = player spawn

function _init()
	   level=1
	   max_level=7
    player={
        x=59,
        y=59,
        w=8,
        h=8,
        dx=0,
        dy=0,
        max_dx=3,
        max_dy=3,
        min_dy=6,
        acc=0.7,
        boost=4,
        satch_boost=5,
        flip=false,
        can_throw=true,
        dead=false,
        startx=0,
        starty=0,
    }
    
    sat_vars={
      max_dx=8,
      max_dy=5,
      force=4,
      fric=0.95,
      boost_range=35,
      dest_range=8,
    }
    
    --x,y,sp,time,w,h
    bricks={
    
    }
    
    crumble_spd=20
    
    target={
      x=0,
      y=0,
      w=8,
      h=8,
      collected=false,
      sp=14,
      anim_spd=4,
      anim_frame=0,
    }
    
    -- dx,dy,x,y,sp,w,h,fix
    sats={}

    gravity=0.3
    fric_ground=0.75
    fric_air=0.90
    
    eol_time=3*30
    eol_frames=3*30
    
    init_level()
    
    debug=false
end
