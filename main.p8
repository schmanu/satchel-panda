-- player
function player_upd()
  player.dy+=gravity
  if (collide_map(player,"down", 0)) then
    player.dx*=fric_ground
  else
    player.dx*=fric_air
  end
  
  player.flip=player.dx<0
end

function player_collide()
 --player collision
 if (player.dy>0 
 and collide_map(player,"down", 0))
 or (player.dy<0 
 and collide_map(player,"up", 1)) then
  player.dy=0
 end
 
 if (player.dx>0 
   and collide_map(player,"right",1))
 or (player.dx<0
   and collide_map(player,"left",1))
 then
   player.dx=0
 end
 
 -- level goal
 if collide_obj(player,target,12)  then
   target.collected=true
 end
 
 -- death
 if collide_map(player,"down",2) 
 or collide_map(player, "up", 3) then
   player.dead=true
 end
end
