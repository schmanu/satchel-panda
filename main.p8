-- controls

function apply_controls()
  -- dead players aint moving
  if player.dead then
    if btnp(4) then
      init_level()
    end
    return
  end
  -- move left
		if (btn(0)) then 
				  player.dx-=player.acc 
		end
		-- move right		
		if btn(1) then 
				player.dx+=player.acc 
		end
		-- throw satchel down
		if btnp(4) and btn(3) then
		  spawn_satch("down")  
		-- throw satchel up
		elseif btnp(4) and btn(2) then
		  spawn_satch("up")
		-- throw satchel left	
		elseif (btnp(4) and player.dx<0) then
		  spawn_satch("left")  
		-- throw satchel right  
		elseif (btnp(4)) then 
		  spawn_satch("right")
		end
		
		-- detonate satchel
		if (btnp(5)) then
		 -- explode oldest satchel
		  for sat in all(sats) do
		    --skip exploding satch
		    if sat.sp==16 then
		      sat.sp+=1
		      -- sound effect
		      sfx(0)
		      -- boost player
		      local px=player.x+4
		      local py=player.y+5
		      
		      local sx=sat.x+2
		      local sy=sat.y+4
		      
		      local dist=abs(px-sx)+abs(py-sy)
		      local dir_x=px-sx
		      local dir_y=py-sy
		      
		      local dir_lgth=sqrt(dir_x^2+dir_y^2) 
		      local f=max(0, player.satch_boost-(player.satch_boost/sat_vars.boost_range)*(dir_lgth-6)) 
		      if f>0 then
		        player.dx=(dir_x/dir_lgth)*f
		        player.dy=(dir_y/dir_lgth)*f
		      end
		      -- break walls
		      for brick in all(bricks) do
		        if brick.sp==57 
		        and collide_obj(brick, sat, sat_vars.dest_range)
		        then
		          -- mark for destruction
		          brick.sp=58
		        end
		      end
		      break 
				  end
				end
			
			player.can_throw=true
		end
end
