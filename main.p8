-- satchel

function satch_upd()
  -- update satchels
  for sat in all(sats) do
	  apply_force(sat)
	  sat_collide(sat)
	  
	  -- move satchel if not fixed
	  if not sat.fix then
	    sat.x+=sat.dx
	    sat.y+=sat.dy
	  end
	  
	  screen_bounds(sat)
	  	  
	  -- explode if sp>16
	  if (sat.sp>16 and sat.sp<20) then
	    sat.sp+=1
	  end
	  
	  if (sat.sp==20) then
	    del(sats,sat)
	  end 
	end
end

function apply_force(sat)
  --applies gravity and force
  if not sat.fix then
   sat.dy+=gravity
	  sat.dx*=sat_vars.fric
  end
end

function spawn_satch(dir)
  -- spawn satch if possible
  if not player.can_throw then
    return
  end
  local off_x=0 local off_y=0
  local dx=0 local dy=0
  if dir=="right" then
    off_x=8
    dx=sat_vars.force
    dy=-1
  elseif dir=="left" then
    dx=-1*sat_vars.force
    dy=-1
  elseif dir=="down" then
    dx=player.dx/sqrt(1+abs(player.dx))
    dy=1.75
    off_y=6
    if player.flip then
      off_x=3
    else
      off_x=1
    end
    
  elseif dir=="up" then
    dx=player.dx/sqrt(1+abs(player.dx))
    dy=-5
    if player.flip then
      off_x=3
    else
      off_x=1
    end
  end
  
  add(sats,{x=player.x+off_x,y=player.y,dx=dx,dy=dy,sp=16,w=5,h=3,fix=false})
  player.can_throw=false
  sfx(2)
end

function sat_collide(sat)
 --satchel collision
 if sat.dy>0 
 and collide_map(sat,"down", 0) then
   sat.dy=0
   sat.fix=true
   sfx(1)
 end
 
 if (sat.dx>0 
   and collide_map(sat, "right",0))
 or (sat.dx<0
   and collide_map(sat, "left", 0))
 then
   sat.dx=0
   sat.fix=true
 end
end
