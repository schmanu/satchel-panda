-- update & draw

function _update()
    player_upd()
    
    satch_upd()
    
    --controls
    apply_controls()
    

    --limit left/right speed
    player.dx=mid(-player.max_dx,player.dx,player.max_dx)
    --limit fall speed
    if (player.dy>0) then
        player.dy=mid(-player.max_dy,player.dy,player.max_dy)
    end
    --limit gain speed
    if (player.dy<0) then
        player.dy=mid(-player.min_dy,player.dy,player.min_dy)
    end


    player_collide()
    
    --apply dx and dy to player position
    player.x+=player.dx
    player.y+=player.dy
    
    -- correct player position
    correct_position(player)
    screen_bounds(player)
    
    --animate goal if collected
    if target.collected then
      if target.anim_frame > 0 then
        target.anim_frame-=1
      else 
		      if target.sp==12 then
		        target.sp=14
		      else
		        target.sp-=1
		      end
		      target.anim_frame=target.anim_spd
      end
    end
    
    -- crumble walls
    crumble()
    
    -- check of next level
    end_level()
end

function _draw()
    cls()
    
    --ground & bricks
    map(16*(level-1),0)
    
    for brick in all(bricks) do
      if brick.sp>0 then
        spr(brick.sp,brick.x,brick.y)
      end
    end
    
    --player
    local player_spr=1
    if not player.can_throw then
      player_spr=4
    end
    
    if player.dead then
      player_spr=5
    end
    
    -- goal
    spr(target.sp,target.x,target.y)
    
    spr(player_spr,player.x,player.y,1,1,player.flip)
    
    -- satchels
    for sat in all(sats) do
      spr(sat.sp,sat.x,sat.y)
    end


	   if debug then
		    --display info
		    print("dx= "..player.dx)
		    print("dy= "..player.dy)
		    print("y= "..player.y)
		  end
end
