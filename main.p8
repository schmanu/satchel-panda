-- brick walls

function crumble()
  for brick in all(bricks) do
    if brick.sp==58 then
      if brick.time == 0 then
        mset(brick.x/8+(level-1)*16,brick.y/8,0)
        brick.sp=0
        -- crumble neighboring bricks
        for other in all(bricks) do
          if other.sp==57 and collide_obj(brick,other,8)
          then
            other.sp=58
          end
        end
      else
        brick.time-=1
      end
    end
    
  end
end
