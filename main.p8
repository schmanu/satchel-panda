-- utils

function screen_bounds(obj) 
  obj.x = max(0,obj.x)
  obj.x = min(15*8,obj.x)
  obj.y = min(15*8,obj.y)
end

function correct_position(obj)
  if not (obj.dy == 0) then
    return
  end
  -- moves object on top of floor
  if collide_map(obj,"down",0) then
    local y2=flr(obj.y)+obj.h-1

    if not (y2%8 == 7) then
      -- player is stuck in ground
      obj.y-=1
    end 
  end
end

-- obj:x,y,w,h,dx,dy
function collide_map(obj,dir,flag)
  local x=obj.x local y=obj.y
  local w=obj.w local h=obj.h
  
  local x1=0 local y1=0
  local x2=0 local y2=0
  
  
  -- collision x
  if (dir=="right") then
		  x1=x+w-1 y1=y+1
		  x2=x+w y2=y+h-2
  elseif (dir=="left") then
    x1=x-1 y1=y+1;
		  x2=x y2=y+h-2;
  elseif (dir=="down") then
    if flag == 2 then
      -- spikes
      x1=x+2 y1=y+h
      x2=x+w-5 y2=y+h
    else 
		    x1=x+2 y1=y+h
		    x2=x+w-2 y2=y+h
    end
    
  elseif (dir=="up") then
    if flag == 3 then
    		--spikes
    		x1=x+2 y1=y+1
		    x2=x+w-5 y2=y+1
    else
		    x1=x+1 y1=y
		    x2=x+w-2 y2=y
    end
  end
  
  --pixels to tiles
  x1=x1/8+(level-1)*16 y1/=8
  x2=x2/8+(level-1)*16 y2/=8
  
  if fget(mget(x1,y1),flag) 
  or fget(mget(x1,y2),flag)
  or fget(mget(x2,y1),flag)
  or fget(mget(x2,y2),flag)
  then
    return true
  end
  
  return false
end

function collide_obj(obj1,obj2,max_dist)
  local x1=obj1.x+obj1.w/2-1
  local x2=obj2.x+obj2.w/2-1
  local y1=obj1.y+obj1.h/2-1
  local y2=obj2.y+obj2.h/2-1
  local dist=sqrt((x1-x2)^2+(y1-y2)^2)
  return dist <= max_dist
end

