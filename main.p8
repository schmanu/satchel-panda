--levels

function end_level()
  if target.collected then
    --end of level timer
    if eol_frames == 0 then
      if level<max_level then
        level+=1
        clear_up()
        init_level()
      end
    else
      eol_frames-=1
    end
    
  end
end

function clear_up()
  bricks={}
  sats={}
end

function init_level()
	-- find and spawn objects
 local off_x=(level-1)*16
 
	for x=0,15 do
	  for y=0,15 do
	    -- spawn player
	    if fget(mget(x+off_x,y),7) do
	      player.startx=x*8
	      player.starty=y*8
	      mset(x+off_x,y,0)
	    end
	    -- spawn target
	    if fget(mget(x+off_x,y),6) do
		    target.x=x*8
		    target.y=y*8
		    mset(x+off_x,y,0)
	    end
	    -- spawn brick walls
	    if fget(mget(x+off_x,y),4) do
		    add(bricks,{x=x*8,y=y*8,sp=57,time=crumble_spd,w=8,h=8})
	    end
	  end
	end
	
	reset_level()
end

function reset_level()
  -- reset states
  player.dead=false
  player.can_throw=true
  target.collected=false
  target.sp=14
  sats={}
  
  -- reset player position
  player.x=player.startx
  player.y=player.starty
  
  -- reset end of level timer
  eol_frames=eol_time
  
  -- reset brick walls
  for brick in all(bricks) do
    brick.sp=57
    brick.time=crumble_spd
    mset(brick.x/8+(level-1)*16,brick.y/8,57)
  end
end
